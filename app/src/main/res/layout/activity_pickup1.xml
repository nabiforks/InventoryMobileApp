<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="20dp"
    tools:context=".Pickup1" >

    <TextView
        android:id="@+id/textView1"
        style="@style/NormalText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="17dp"
        android:text="@string/pickup1_header_text" />

    <LinearLayout
        android:id="@+id/pickup_info1"
        android:layout_width="550dp"
        android:layout_height="230dp"
        android:layout_alignLeft="@+id/autoCompleteTextView1"
        android:layout_alignRight="@+id/btnPickup1Cont"
        android:layout_below="@+id/autoCompleteTextView1"
        android:layout_marginTop="10dp" >

        <TableLayout
            android:id="@+id/pickup_info1a"
            android:layout_width="550dp"
            android:layout_height="wrap_content"
            android:layout_gravity="top|right"
            android:background="@drawable/customshape" >

            <TableRow>

                <TextView
                    android:id="@+id/tvHeadDetails1"
                    style="@style/NormalText"
                    android:layout_gravity="center"
                    android:layout_span="2"
                    android:gravity="center"
                    android:text="Pickup Location"
                    android:textColor="@color/bluedark" />
            </TableRow>

            <TableRow>

                <TextView
                    android:id="@+id/tvLabelOffice1"
                    style="@style/NormalText"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="right"
                    android:text="Resp Center Hd:" />

                <TextView
                    android:id="@+id/tvOffice1"
                    style="@style/NormalText"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="left"
                    android:text="N/A" />
            </TableRow>

            <TableRow>

                <TextView
                    android:id="@+id/tvLabelDescr1"
                    style="@style/NormalText"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="right"
                    android:text="Address:" />

                <TextView
                    android:id="@+id/tvDescript1"
                    style="@style/NormalText"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="left"
                    android:text="N/A" />
            </TableRow>

            <TableRow>

                <TextView
                    android:id="@+id/tbLabelCount1"
                    style="@style/NormalText"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="right"
                    android:text="Item Count:" />

                <TextView
                    android:id="@+id/tvCount1"
                    style="@style/NormalText"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="left"
                    android:text="N/A" />
            </TableRow>
        </TableLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/pickup_info2"
        android:layout_width="550dp"
        android:layout_height="230dp"
        android:layout_alignLeft="@+id/autoCompleteTextView2"
        android:layout_alignRight="@+id/autoCompleteTextView2"
        android:layout_below="@+id/autoCompleteTextView2"
        android:layout_marginTop="10dp" >

        <TableLayout
            android:id="@+id/pickup_info2A"
            android:layout_width="550dp"
            android:layout_height="wrap_content"
            android:background="@drawable/customshape" >

            <TableRow>

                <TextView
                    android:id="@+id/tvHeadDetails2"
                    style="@style/NormalText"
                    android:layout_gravity="center"
                    android:layout_span="2"
                    android:gravity="center"
                    android:text="Delivery Location"
                    android:textColor="@color/bluedark" />
            </TableRow>

            <TableRow>

                <TextView
                    android:id="@+id/tvLabelOffice2"
                    style="@style/NormalText"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="right"
                    android:text="Resp Center Hd:" />

                <TextView
                    android:id="@+id/tvOffice2"
                    style="@style/NormalText"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="left"
                    android:text="N/A" />
            </TableRow>

            <TableRow>

                <TextView
                    android:id="@+id/tvLabelDescr2"
                    style="@style/NormalText"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="right"
                    android:text="Address:" />

                <TextView
                    android:id="@+id/tvDescript2"
                    style="@style/NormalText"
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="left"
                    android:text="N/A" />
            </TableRow>

            <TableRow>

                <TextView
                    android:id="@+id/tbLabelCount2"
                    style="@style/NormalText"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:gravity="right"
                    android:text="Item Count:" />

                <TextView
                    android:id="@+id/tvCount2"
                    style="@style/NormalText"
                    android:layout_marginTop="10dp"
                    android:gravity="left"
                    android:text="N/A" />
            </TableRow>
        </TableLayout>
    </LinearLayout>

    <gov.nysenate.inventory.android.ClearableAutoCompleteTextView
        android:id="@+id/autoCompleteTextView2"
        style="@style/NormalText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/pickup_info1"
        android:layout_marginTop="5dp"
        android:ems="10"
        android:hint="@string/select_dest_loc"
        android:nextFocusDown="@+id/autoCompleteTextView1"
        android:singleLine="true"
        android:textStyle="bold" >
    </gov.nysenate.inventory.android.ClearableAutoCompleteTextView>

    <Button
        android:id="@+id/btnPickup1Cancel"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignBaseline="@+id/btnPickup1Cont"
        android:layout_alignBottom="@+id/btnPickup1Cont"
        android:layout_alignParentLeft="true"
        android:background="@drawable/buttonshapered"
        android:onClick="cancelButton"
        android:shadowColor="#000000"
        android:shadowDx="01"
        android:shadowDy="1"
        android:shadowRadius="5"
        android:text="Cancel"
        android:textColor="#FFFFFF"
        android:textSize="30sp" />

    <Button
        android:id="@+id/btnPickup1Cont"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="36dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/buttonshapegreen"
        android:onClick="continueButton"
        android:shadowColor="#000000"
        android:shadowDx="01"
        android:shadowDy="1"
        android:shadowRadius="5"
        android:text="Continue"
        android:textColor="#FFFFFF"
        android:textSize="30sp" />

    <ProgressBar
        android:id="@+id/progBarPickup1"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/shaperoundblue"
        android:visibility="invisible" />

    <gov.nysenate.inventory.android.ClearableAutoCompleteTextView
        android:id="@+id/autoCompleteTextView1"
        style="@style/NormalText"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_alignRight="@+id/btnPickup1Cont"
        android:layout_below="@+id/textView1"
        android:ems="10"
        android:hint="@string/select_origin_loc"
        android:nextFocusDown="@+id/autoCompleteTextView2"
        android:singleLine="true"
        android:textStyle="bold" >

        <requestFocus />
    </gov.nysenate.inventory.android.ClearableAutoCompleteTextView>

</RelativeLayout>